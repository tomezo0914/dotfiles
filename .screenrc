defutf8 on
defencoding utf8
encoding utf-8 utf-8

escape ^Tt
bind w windowlist -b

vbell off
startup_message off
autodetach on
defscrollback 1024

caption always "%{= wg} %-w%{=bu dr}%n %t%{-}%+w %= %{=b wb}%y/%m/%d %{=b wm}%c"
#hardstatus on
#hardstatus alwayslastline '%m/%d %02c:%s %{= .g}%H%{-} %L=%-w%45L>%{=u g.}%n %t%{-}%+w %-17<%=%{= .y}(%l)'

defbce on
term rxvt-256color
termcapinfo xterm* ti@:te@
shell ${SHELL}
